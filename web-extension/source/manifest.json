{
	"name": "Taqche Extension",
	"version": "0.0.0",
	"description": "An awesome new browser extension",
	"homepage_url": "https://github.com/ha-D/taqche",
	"author": "Hadi Zol",
	"manifest_version": 2,
	"minimum_chrome_version": "74",
	"applications": {
		"gecko": {
			"id": "taqche@hadi.sh",
			"strict_min_version": "67.0"
		}
	},
	"icons": {
		"128": "icon.png"
	},
	"permissions": [
		"<all_urls>"
	],
	"content_scripts": [
		{
			"matches": ["https://www.youtube.com/watch*"],
			"js": ["youtube.js"],
			"run_at": "document_end"
		}
	]
}
