{
	"name": "Taqche Extension",
	"version": "0.0.0",
	"description": "An awesome new browser extension",
	"homepage_url": "https://github.com/ha-D/taqche",
	"author": "Hadi Zol",
	"manifest_version": 2,
	"minimum_chrome_version": "74",
	"icons": {
		"128": "icon.png"
	},
	"permissions": [
		"<all_urls>",
		"storage"
	],
	"content_scripts": [
		{
			"matches": ["https://www.youtube.com/watch*"],
			"js": ["youtube.js"],
			"run_at": "document_idle"
		}
	],
	"background": {
		"scripts": ["background.js"]
	},
	"options_ui": {
		"page": "options.html"
	}
}
